{
  "Comment": "Reads Echo Items on the SQS queue and rebuilds the respective AWS resources",
  "StartAt": "CheckSQS",
  "States": {
    "CheckSQS": {
      "Type": "Task",
      "Resource": "${function_arn}",
      "Parameters": {
        "TaskName": "CheckSQS",
        "QueueUrl": "${queue_url}"
      },
      "End": true
    }
  }
}